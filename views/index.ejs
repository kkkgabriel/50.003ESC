<!DOCTYPE HTML>
<html lang="en" ng-controller="mainController" ng-app="telco"> <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <title>My First Rainbow Application</title>
        
        <script src="//cdnjs.cloudflare.com/ajax/libs/es5-shim/4.5.9/es5-shim.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/es6-promise/4.0.5/es6-promise.min.js"></script>
        <script src="//code.jquery.com/jquery-2.1.3.min.js"></script>
        <script src="//cdn.jsdelivr.net/momentjs/2.15.1/moment-with-locales.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.7.5/angular.min.js"></script>
        <script src="./assets/js/lib/vendors-sdk.min.js"></script>
        <script src="./assets/js/lib/rainbow-sdk.min.js"></script>

        <!-- main controller -->
        <script src="./app/app.js"></script>

        <!-- components -->
        <!-- <script src="./assets/js/login/login.js"></script> -->
    </head>
    <body>
        <!-- <script src="/assets/js/loader-sdk.min.js" data-main="./app/config.json"></script> -->
        
        <h1>gabrielrulz</h1>    
        <div>
            <h2>Messages</h2>
            <div style="height: 200px; border-style:solid; overflow: scroll;">
                <p ng-repeat="message in conversation.messages">{{message.from.initials}}: {{message.data}}</p>
            </div>
            <input type="text" ng-model="draft" placeholder="Enter your message here" style="width: 200px;"/>
            <button ng-click="sendMsg()">Send</button>
        </div>
        
    </body>
</html>
